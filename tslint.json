{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "linterOptions": {
    "exclude": [
      "src/**.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name" : false,
    "max-line-length": false,
    "no-console": false
  }
}
