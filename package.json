{
  "name": "typescript-react-component-library",
  "version": "0.1.0",
  "description": "TypeScript React component library and demo application",
  "author": "Brian Gebel <brian@pixeloven.com>",
  "license": "MIT",
  "homepage": "https://github.com/pixeloven/typescript-react-component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixeloven/typescript-react-component-library.git"
  },
  "keywords": [
    "react",
    "semantic",
    "components",
    "typescript"
  ],
  "dependencies": {
    "@pixeloven/cli": "0.5.1",
    "@pixeloven/core": "0.5.1",
    "@pixeloven/tasks": "0.5.1",
    "axios": "0.18.0",
    "deepmerge": "2.2.1",
    "express": "4.16.4",
    "express-winston": "3.0.1",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "object-assign": "4.1.1",
    "promise": "8.0.2",
    "moment": "2.22.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-universal-component": "3.0.3",
    "redux": "4.0.1",
    "redux-form": "7.4.2",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "semantic-ui-react": "0.83.0",
    "semantic-ui-sass": "2.3.1",
    "whatwg-fetch": "3.0.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.5",
    "@storybook/addon-actions": "4.0.2",
    "@storybook/addon-knobs": "4.0.2",
    "@storybook/addon-notes": "4.0.2",
    "@storybook/addon-options": "4.0.2",
    "@storybook/addons": "4.0.2",
    "@storybook/core": "4.0.2",
    "@storybook/react": "4.0.2",
    "@types/classnames": "2.2.6",
    "@types/enzyme": "^3.1.0",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.0",
    "@types/express-winston": "^3.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/lodash": "^4.14.115",
    "@types/node": "^8.10.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-helmet": "^5.0.7",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.32",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.16",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-notes": "^3.3.3",
    "@types/storybook__addon-options": "^3.2.2",
    "@types/storybook__react": "^3.0.9",
    "babel-loader": "8.0.4",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "husky": "1.1.4",
    "lint-staged": "7.3.0",
    "node-sass": "4.9.4",
    "npm-run-all": "4.1.3",
    "raf": "3.4.0",
    "redux-devtools-extension": "2.13.5",
    "resolve": "1.8.1",
    "storybook-readme": "4.0.2",
    "ts-node": "7.0.1",
    "webpack": "4.25.1",
    "webpack-flush-chunks": "2.0.3"
  },
  "devEngines": {
    "node": "8.x || 9.x || 10.x"
  },
  "scripts": {
    "build": "run-p build:app build:story",
    "build:app": "pixeloven-tasks build",
    "build:story": "build-storybook -c .storybook -o build/public/docs",
    "deploy": "node build/server.js",
    "lint": "pixeloven-cli lint:ts && pixeloven-cli lint:scss",
    "lint:ts": "pixeloven-cli lint:ts",
    "lint:scss": "pixeloven-cli lint:scss",
    "prebuild": "rm -rf ./build",
    "precommit": "lint-staged",
    "pretest": "pixeloven-cli test:clean",
    "pretty": "pixeloven-cli pretty",
    "pretty:ts": "pixeloven-cli pretty:ts",
    "pretty:scss": "pixeloven-cli pretty:scss",
    "start": "run-p start:app start:story",
    "start:app": "pixeloven-tasks serve",
    "start:story": "start-storybook --quiet -s ./public -p 9001 -c .storybook",
    "test": "pixeloven-cli test",
    "test:watch": "pixeloven-cli test:watch"
  },
  "lint-staged": {
    "src/**/*.{scss,ts,tsx}": [
      "yarn pretty",
      "git add"
    ],
    "src/**/*.{scss}": [
      "yarn pretty:scss",
      "yarn lint:scss",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "yarn pretty:ts",
      "yarn lint:ts",
      "git add"
    ]
  }
}
