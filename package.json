    "@types/storybook__addon-notes": "^3.3.0",
    "@types/storybook__addon-options": "^3.2.0",
    "@types/storybook__react": "^3.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.2",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "moment": "^2.22.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "storybook-readme": "^4.0.0-beta1",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.20.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "storybook": "yarn build-css && start-storybook -p 9001 -c .storybook",
    "storybook:build": "yarn build-css && build-storybook -c .storybook -o public/docs"
  }
}
